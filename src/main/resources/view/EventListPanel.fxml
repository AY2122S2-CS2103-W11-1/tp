<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="header" styleClass="cell_bigger_label" text="\\$header" textAlignment="CENTER">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></Label>
    <ListView fx:id="eventListView" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets />
        </VBox.margin></ListView>
</VBox>
